using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

using System;
using System.Runtime.InteropServices.WindowsRuntime;
using System.CodeDom;
namespace Actividad_1_AppConsola
{
    internal class Program
    {
        static void Main(string[] args)
        {
            //Vamos a declarar las variables princiapalmente
            string nombre;
            int edad;
            string fechaNac;
            string carrera;
            string final; //Para marcar un final

            Console.WriteLine("Bienvenidos a UMI/Universidad Coppel");
            Console.WriteLine("\nNombre completo:");
            nombre = Console.ReadLine();
            Console.WriteLine("\nEdad: ");
            edad = int.Parse(Console.ReadLine());
            Console.WriteLine("\nIngresa fecha de nacimiento en formato DD/MM/YYYY: ");
            fechaNac = Console.ReadLine();
            Console.WriteLine("\nCarrera a la que desea ingresar: ");
            carrera = Console.ReadLine();

            Console.WriteLine("\n\nGracias " + nombre + " por formar parte de UMI/Universidad Coppel, Bienvenido a la carrera de " + carrera+".");
            Console.WriteLine("\nHecho por: Bruno Alberto German Meza UMI");
            final = Console.ReadLine();





using System;
using System.Collections.Generic;
using System.ComponentModel;
using System.Data;
using System.Drawing;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Windows.Forms;
using WindowsFormsActividad_2.Formularios;

namespace WindowsFormsActividad_2
{
    public partial class FormMenu : Form
    {
        public FormMenu()
        {
            InitializeComponent();
        }

        private void datosPersonalesToolStripMenuItem_Click(object sender, EventArgs e)
        {
            FormDatosPersonales datos = new FormDatosPersonales();
            datos.Show();
            this.Hide();
        }

        private void salirToolStripMenuItem_Click(object sender, EventArgs e)
        {
            Application.Exit();

        }

        private void saludoToolStripMenuItem_Click(object sender, EventArgs e)
        {
            FormSaludo saludo = new FormSaludo();
            saludo.Show();
            this.Hide();
        }

        private void operacionesBasicasToolStripMenuItem_Click(object sender, EventArgs e)
        {
            FormOperacionesBasicas basicas = new FormOperacionesBasicas();
            basicas.Show();
            this.Hide();
        }

        private void FormMenu_Load(object sender, EventArgs e)
        {

        }

        private void FormMenu_FormClosing(object sender, FormClosingEventArgs e)
        {
            Application.Exit();

        }
    }
}
