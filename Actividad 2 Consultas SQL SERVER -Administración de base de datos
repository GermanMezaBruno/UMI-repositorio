DROP TABLE Empleados --BORRA TABLA COMPLETA
DROP TABLE Sucursales --BORRA TABLA COMPLETA



-- Voy a practicar el uso de la foreing key como lo enseño el profesor Miguel.



CREATE TABLE Sucursales (
numero_sucursal INT PRIMARY KEY,
nombre_sucursal VARCHAR (40),
nombre_encargado VARCHAR (40),
direccion_sucursal VARCHAR (50),
telefono_sucursal VARCHAR (10), --Utilizo la recomendación del profesor Miguel Angel Rodriguez donde pones 10 digitos para el celular ahorrando 1 caracter con el int
ciudad_sucursal VARCHAR (20),
estado_sucursal VARCHAR (20)
)

CREATE TABLE Empleados (
nombre_empleado varchar (40) PRIMARY KEY,
direccion_empleado varchar (40),
telefono_empleado VARCHAR (10),
edad_empleado INT,
numero_sucursal INT,
correo_empleado VARCHAR (30),
contraseña VARCHAR (20),
nombre_encargado varchar (40),
);

--Ahora voy a hacer las relaciones de PK y FK
ALTER TABLE Empleados --Se pone la tabla que vas a agregarle la FK
ADD CONSTRAINT FK_Empleados_Sucursales --FK_TABLA DONDE ESTA LA FK_ TABLA DONDE ESTA LA PK
FOREIGN KEY (numero_sucursal) REFERENCES Sucursales (numero_sucursal) -- Tipo y a quien convertiras en FK y referencia (de donde viene tabla y columna a convertir)
ON UPDATE CASCADE
ON DELETE CASCADE




--CONSULTAS--
SELECT * FROM categoria
SELECT * FROM cliente
SELECT * FROM detalle
SELECT * FROM Empleados
SELECT * FROM factura
SELECT * FROM producto
SELECT * FROM Sucursales



--1.- Seleccionar los clientes del año 2021 en base a la fecha de la factura
SELECT * FROM factura
WHERE YEAR (fecha) = 2021
ORDER BY fecha ASC;

--2.- Seleccionar los clientes de 2022 (hasta el momento) en base a la fecha de la factura.
SELECT * FROM factura
WHERE YEAR (fecha) = 2022
ORDER BY fecha ASC;

--3.- Seleccionar los clientes de diciembre de 2021.
SELECT * FROM factura
WHERE MONTH (fecha) = 12 
AND YEAR (fecha) = 2021
ORDER BY fecha ASC;

--4.- ¿Qué compras han realizado los siguientes clientes?:
--Valentina Anastasia Huerta Corral
--Zayra Manuela Gómez López
--Dante Eduardo Dolores Meza
--Ana Maribel Cedillo Núñez 
--Rodrigo Ismael Silva Ugarte

SELECT c.nombre, --Seleccionas las tabalas que vas unir
c.apellido,
f.id_factura,
p.id_producto,
p.nombre
FROM cliente c  --Tabla principal
INNER JOIN factura f ON c.id_cliente = f.id_cliente --Uni la tabla factura para pasar las facturas de cada cliente
INNER JOIN detalle d ON d.id_detalle = f.id_detalle  --Uni la tabla detalle para hacer puente con la de producto, donde sacaría el nombre de cada producto
INNER JOIN producto p ON p.id_producto = d.id_detalle --Uni la tabla producto para poder anexar el nombre de esta tabla
WHERE c.id_cliente IN (2, 17, 24,4,11) --estos son los ID de cada cliente respectivamente
ORDER BY c.nombre ASC, id_factura ASC;

SELECT * FROM cliente
SELECT * FROM detalle
SELECT * FROM producto
SELECT * FROM factura



--5.- Seleccionar el producto que más ventas ha tenido.
SELECT id_producto, COUNT(*) AS veces_vendido
FROM Detalle
GROUP BY id_producto
ORDER BY veces_vendido DESC;

SELECT p.nombre, d.id_producto, COUNT(*) AS veces_vendido
FROM detalle d
JOIN producto p ON d.id_producto = p.id_producto
GROUP BY p.nombre, d.id_producto
ORDER BY veces_vendido DESC;

ALTER TABLE producto
ALTER COLUMN nombre VARCHAR(150) --Se cambio porque estaba en un tipo de dato incorrecto para hacer la consulta

sp_help producto --verifique el tipo de dato



--6.- ¿Qué producto tienen más cantidad en stock?
SELECT * FROM producto
ORDER BY stock DESC;

--7.- Ordenar, de la más antigua a la más reciente, las compras que ha habido en la tienda.
SELECT * FROM factura
ORDER BY fecha ASC;

--8.- Ordenar alfabéticamente los nombres de todos los clientes de la tienda.
ALTER TABLE cliente
ALTER COLUMN nombre NVARCHAR(200); --Se cambio el tipo de dato a NVARCHAR ya que me daba erro cuando hacia un SELECT en nombre.

SELECT * FROM cliente
ORDER BY nombre ASC;

--9.- Seleccionar cuáles productos pertenecen a cada categoría: 
--Falda 
--Pantalón 
--Chamarra
--Zapatos
--Accesorios

SELECT p.id_categoria,--Seleccionas las tabalas que vas unir
p.nombre, 
p.id_producto,
c.nombre,
c.descripcion
FROM producto p  --Tabla principal
INNER JOIN categoria c ON p.id_categoria = c.id_categoria -- Se hace la conexión entre tablas con la id_categoria de producto y la id_categoria de categoria.
WHERE c.nombre IN ('Falda', 'Zapato', 'Pantalón', 'Chamarra','Accesorios') --Seleccionas el nombre de la categoria que quieres organizar
ORDER BY p.id_producto ASC  --Por ultimo lo organizas de forma ascendente por medio del id_producto.

SELECT * FROM producto
SELECT * FROM categoria

sp_help categoria --verifique el tipo de dato

ALTER TABLE categoria
ALTER COLUMN nombre VARCHAR(150)--Se cambio porque estaba en un tipo de dato incorrecto para hacer la consulta en el ejercicio 9, tipo de dato texto se cambio a varchar.


--10.- Seleccionar los encargados de las sucursales de la tienda Akira’s Boutique
SELECT  nombre_sucursal, nombre_encargado  
FROM Sucursales

--11.- Seleccionar los empleados que trabajan en la sucursal de Akira’s Boutique: Constitución
SELECT  nombre_empleado, numero_sucursal, nombre_encargado
FROM Empleados
WHERE numero_sucursal= 6

--12.- ¿Qué clientes son mayores de 30 años?
SELECT * FROM cliente
WHERE edad

SELECT *
FROM cliente
WHERE fec_nac <= '1995-09-24'
