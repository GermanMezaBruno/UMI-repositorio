ALTER TABLE Empleados
ALTER COLUMN direccion_empleado VARCHAR (40);

ALTER TABLE Empleados
ALTER COLUMN correo_empleado VARCHAR (40); --Se cambio el número de caracteres porque me daba error al introducir el correo, se quedaba corto en caracteres.

ALTER TABLE Empleados
ADD Nombre_encargado_suc VARCHAR (40);

ALTER TABLE Sucursales
ALTER COLUMN direccion_sucursal VARCHAR (50); --Se cambio el número de caracteres porque me daba error al introducir la calle, se quedaba corto en caracteres.

EXEC sp_rename 'Empleados.Nombre_encargado_suc', 'nombre_encargado_suc', 'COLUMN';

CREATE TABLE Sucursales (
numero_sucursal INT PRIMARY KEY,
nombre_sucursal VARCHAR (40),
nombre_encargado VARCHAR (40),
direccion_sucursal VARCHAR (50),
telefono_sucursal VARCHAR (10), --Utilizo la recomendación del profesor Miguel Angel Rodriguez donde pones 10 digitos para el celular ahorrando 1 caracter con el int
ciudad_sucursal VARCHAR (20),
estado_sucursal VARCHAR (20)
)

CREATE TABLE Empleados (
nombre_empleado varchar (40) PRIMARY KEY,
direccion_empleado varchar (40),
telefono_empleado VARCHAR (10),
edad_empleado INT,
numero_sucursal INT,
correo_empleado VARCHAR (30),
contraseña VARCHAR (20),
nombre_encargado varchar (40),
FOREIGN KEY (numero_sucursal) REFERENCES Sucursales(numero_sucursal)
);

ALTER TABLE Empleados
ALTER COLUMN direccion_empleado VARCHAR (40);

ALTER TABLE Empleados
ALTER COLUMN correo_empleado VARCHAR (40); --Se cambio el número de caracteres porque me daba error al introducir el correo, se quedaba corto en caracteres.

ALTER TABLE Empleados
ADD Nombre_encargado_suc VARCHAR (40);

ALTER TABLE Sucursales
ALTER COLUMN direccion_sucursal VARCHAR (50); --Se cambio el número de caracteres porque me daba error al introducir la calle, se quedaba corto en caracteres.

EXEC sp_rename 'Sucursales.Estado_sucursal','estado_sucursal','COLUMN'; --Cambiar nombre de columna= 'TABLA.COLUMNA', 'NOMBRE NUEVO', 'ES UNA COLUMNA';


